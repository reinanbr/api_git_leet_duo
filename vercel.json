{
    "version": 2,
    "builds": [
        {
            "src": "api/git/git_contrib.go",
            "use": "@vercel/go"
        },
        {
            "src": "api/git/git_user.go",
            "use": "@vercel/go"
        },
        {
            "src": "api/git/git_langs.go",
            "use": "@vercel/go"
        }
    ],
    "rewrites": [
        {
            "source": "/api/git/git_contrib",
            "destination": "api/git/git_contrib.go"
        },
        {
            "source": "/api/git/git_user",
            "destination": "api/git/git_user.go"
        },{
            "source": "/api/git/git_langs",
            "destination": "api/git/git_langs.go"
        },
        
        {
            "source": "/doc/(.*)",
            "destination": "public/"
        }
    ],
    "cleanUrls": true
}